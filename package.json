{
  "name": "koa2-qbgoods",
  "version": "0.0.1",
  "author": "sean",
  "scripts": {
    "start": "better-npm-run dev",
    "test": "better-npm-run test",
    "product": "better-npm-run production",
    "clear": "rm -rf dist/",
    "compile-test": "npm run clear && better-npm-run compile-test",
    "compile": "npm run clear && babel src/ --out-dir dist/ --retain-lines --source-maps"
  },
  "betterScripts": {
    "dev": {
      "command": "node -r dotenv/config bin/index",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "compile-test": {
      "command": "babel src/ --out-dir dist/ --retain-lines --source-maps",
      "env": {
        "NODE_ENV": "test"
      }
    },
    "test": {
      "command": "pm2 start dist/index"
    },
    "production": {
      "command": "node -r dotenv/config bin/index ",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "engines": {
    "node": ">= 4"
  },
  "dependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-1": "^6.22.0",
    "babel-preset-stage-3": "^6.22.0",
    "babel-runtime": "^6.23.0",
    "better-npm-run": "0.0.14",
    "chokidar": "^1.6.1",
    "co-redis": "^2.1.1",
    "debug": "^2.6.1",
    "dotenv": "^4.0.0",
    "es6-promise": "^4.1.0",
    "koa": "^2.0.0",
    "koa-bodyparser": "^2.0.1",
    "koa-convert": "^1.2.0",
    "koa-json": "^1.1.3",
    "koa-logger": "^2.0.1",
    "koa-onerror": "^3.0.2",
    "koa-router": "^7.0.0",
    "lodash": "^4.17.4",
    "redis": "^2.6.5",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "module-alias": "^2.0.0"
  },
  "_moduleAliases": {
    "@root": ".",
    "QBFK": "dist/library/framework/index.js",
    "model/dictionary": "dist/model/dictionary/index.js",
    "model/return": "dist/model/basic/return.js",
    "lib/config": "dist/library/config/index.js",
    "lib/helper": "dist/library/helper/index.js",
    "lib/redis": "dist/library/util/redis.js",
    "lib/error": "dist/library/util/error.js"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "license": "MIT"
}
